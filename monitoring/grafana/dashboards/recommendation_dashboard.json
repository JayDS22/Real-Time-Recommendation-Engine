{
  "dashboard": {
    "id": null,
    "title": "Real-Time Recommendation Engine Dashboard",
    "tags": ["recommendation", "ml", "performance"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "API Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "95th Percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(api_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "50th Percentile"
          }
        ],
        "yAxes": [
          {
            "unit": "ms",
            "min": 0,
            "max": 200
          }
        ],
        "thresholds": [
          {
            "value": 100,
            "colorMode": "critical",
            "op": "gt"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "yAxes": [
          {
            "unit": "reqps",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Model Performance Metrics",
        "type": "singlestat",
        "targets": [
          {
            "expr": "recommendation_quality_bucket{metric=\"ndcg_10\"}",
            "legendFormat": "NDCG@10"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "thresholds": "0.7,0.78",
        "colorBackground": true,
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / rate(cache_requests_total[5m])",
            "legendFormat": "Hit Rate"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "thresholds": "0.8,0.9",
        "colorBackground": true,
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8}
      },
      {
        "id": 5,
        "title": "Active Users",
        "type": "singlestat",
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "Active Users"
          }
        ],
        "valueName": "current",
        "format": "short",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "rate(api_requests_total{status=~\"5..\"}[5m]) / rate(api_requests_total[5m])",
            "legendFormat": "Error Rate"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "thresholds": "0.01,0.05",
        "colorBackground": true,
        "colorValue": true,
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8}
      },
      {
        "id": 7,
        "title": "Recommendation Quality Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "recommendation_quality_bucket{metric=\"ndcg_10\"}",
            "legendFormat": "NDCG@10"
          },
          {
            "expr": "recommendation_quality_bucket{metric=\"map_10\"}",
            "legendFormat": "MAP@10"
          },
          {
            "expr": "recommendation_quality_bucket{metric=\"hit_rate_20\"}",
            "legendFormat": "Hit Rate@20"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memory Usage"
          },
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU Usage %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 9,
        "title": "A/B Test Results",
        "type": "table",
        "targets": [
          {
            "expr": "ab_test_lift_percentage",
            "legendFormat": "CTR Lift %",
            "format": "table"
          },
          {
            "expr": "ab_test_p_value",
            "legendFormat": "P-Value",
            "format": "table"
          },
          {
            "expr": "ab_test_statistical_power",
            "legendFormat": "Statistical Power",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "10s",
    "version": 1
  }
}
